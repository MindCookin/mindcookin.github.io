{
  "name": "mindcookin.github.io",
  "version": "1.0.0",
  "description": "Mind Cookin's portfolio",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./js/**/*.js",
    "build": "browserify -d -e index.js -t babelify -o dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MindCookin/mindcookin.github.io"
  },
  "author": "Santiago Bernab√©",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MindCookin/mindcookin.github.io/issues"
  },
  "transform": [
    [
      "babelify",
      {
        "blacklist": [
          "regenerator"
        ]
      }
    ]
  ],
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "homepage": "https://github.com/MindCookin/mindcookin.github.io",
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-eslint": "^3.1.17",
    "babel-jest": "^5.3.0",
    "babelify": "^6.1.2",
    "browserify": "^10.2.3",
    "eslint": "^0.23.0",
    "eslint-plugin-react": "^2.5.2",
    "jest-cli": "^0.4.13",
    "react": "^0.13.3"
  }
}
